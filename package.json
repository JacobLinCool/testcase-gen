{
    "name": "testcase-gen",
    "version": "0.2.1",
    "description": "Testcase Generator",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "bin":{
        "testcase-gen": "lib/cli.js",
        "tc-gen": "lib/cli.js"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "start": "ts-node -- ./src/",
        "prepare": "npm run build && npm run doc",
