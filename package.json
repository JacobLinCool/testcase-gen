    },
    "devDependencies": {
        "@types/node": "^16.10.9",
        "nodemon": "^2.0.13",
        "prettier": "^2.4.1",
        "ts-node": "^10.3.0",
        "typedoc": "^0.22.5",
        "typescript": "^4.4.4"
    },
    "bugs": {
      "url": "https://github.com/JacobLinCool/testcase-gen/issues"
    },
